       <name>Imjacin</name>
       <url>http://dev.imjac.in/maven/</url>
     </repository>
  </repositories>
  
  <dependencies>
    <dependency>
      <groupId>edu.wpi.first.wpilibj</groupId>
      <artifactId>wpilibj-java</artifactId>
      <version>2018.1.1-beta-2-20171109025718-20-g7efab4c</version>
